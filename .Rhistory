library(openintro)
library(tidyverse)
library(infer)
school_data <- tibble(
method = c(rep(c("A", "B"), c(200, 180))),
grade = c(rep(c("Passing", "Not Passing"), c(120, 80)),
rep(c("Passing", "Not Passing"), c(100, 80)))
)
school_data |>
table()
# Creating a bar plot to summarize the observed data distribution
school_data |> ggplot(aes(x=method, fill = grade)) +
geom_bar() +
theme_bw()
p_hats <- school_data |>
group_by(method) |>
summarize(estimates = mean(grade == "Passing", na.rm = TRUE))
p_hats
d_hat <- diff(p_hats$estimates)
d_hat
.6 - .56
set.seed(62)
null_dist <- school_data |>
specify(
response = grade,
explanatory = method,
success = "Passing"
) |>
hypothesize(null = "independence") |>
generate(reps = 1000, type = "permute")|>
calculate(stat = "diff in props",
order = c("B", "A"))
null_dist
null_dist |> visualize() +
geom_vline(xintercept = d_hat, color = "red")
null_dist |> get_p_value(obs_stat = d_hat, direction = "two-sided")
null_dist |> visualize() +
shade_p_value(obs_stat = d_hat, direction = "two-sided")
